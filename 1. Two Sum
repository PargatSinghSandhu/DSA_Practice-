class Solution 
{
public:
    vector<int> twoSum(vector<int>& nums, int target) 
    {

         map<int, int> mpp;
         for(int i=0;i<nums.size();i++)
         {
             int first_element = nums[i];
             int second_element = target - first_element;
             if(mpp.find(second_element)!=mpp.end())
             {
                 return {mpp[second_element], i};
             }
             mpp[first_element] = i;

         }
         
         return {-1,-1};
    } 
    
};
