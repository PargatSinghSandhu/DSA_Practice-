class Solution {
public:
    int search(vector<int>& nums, int target) {
        
        int low = 0, high = nums.size()-1;
       
        while(low<=high)
        {
            int mid_i  = (low+high)/2;
            int mid = nums[mid_i];
            if(target == mid)
            {
                return mid_i;
            }
            else 
            if(target>mid)
            {
                low = mid_i+1;
            }
            else
            {
                high = mid_i-1;
            }

        }
        return -1;

    }
};
